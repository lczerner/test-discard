set terminal postscript enhanced color
set output "graphs.ps"
set style data line
set y2tics
set xlabel "Record size [kB]"
set ylabel "Duration [s]"
set y2label "Throughput [MB/s]"
set ytics nomirror
set xtics nomirror
set title "Average duration of discard operations and Throughput in MB/s\n\
depending on record size"
plot "output.dat" using ($1/1024):5 title "Average duration", \
"output.dat" using ($1/1024):7 axes x1y2 title "Throughput"
set title "Summary of duration of discard operations and Throughput in MB/s\n\
depending on record size"
plot "output.dat" using ($1/1024):6 title "Duration Summary", \
"output.dat" using ($1/1024):7 axes x1y2 title "Throughput"
set title "Minimal and maximal durations depending on record size"
unset y2tics
unset y2label
plot "output.dat" using ($1/1024):4 title "Max", \
"output.dat" using ($1/1024):3 title "Min", \
"output.dat" using ($1/1024):5 title "Average duration"
